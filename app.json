{
  "expo": {
    "name": "Realto",
    "slug": "Realto",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dejavuapps.realtoapp",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSContactsUsageDescription": "Realto want to access your contacts to to add your listing",
        "NSPhotoLibraryUsageDescription": "Realto want to access your photos to add them in your listing"
      }
    },
    "android": {
      "package": "com.dejavuapps.realtoapp",
      "permissions": [
        "READ_CONTACTS",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}